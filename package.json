{
  "name": "kids-projects",
  "private": true,
  "version": "0.0.1",
  "description": "Simple multi-project repo for kids with live reload dev server",
  "scripts": {
    "gen:index": "node scripts/generate-index.mjs",
    "dev": "npm run gen:index && npx -y live-server --port=5173",
    "dev:piano": "npm run gen:index && npx -y live-server --port=5173 --open=projects/piano/",
    "build": "npm run gen:index"
  }
}
